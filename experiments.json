{
    "BASE_CIFAR10": {
        "files": {
            "save_dir": "results/BASE_CIFAR10"
        },
        "dataset": {
            "name": "CIFAR10",
            "transform": {
                "size": 32
            },
            "input_dims": [
                3,
                32,
                32
            ]
        },
        "hyperparameters": {
            "lam_recon_comment, explains the math": "0.0005 * 32*32*3",
